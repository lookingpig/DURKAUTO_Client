<?xml version="1.0" encoding="UTF-8"?>
<message-services>
	<!-- 更换消息通讯密匙服务 -->
	<service>
		<key>ChangeMessageKeyService</key>
		<class>org.lookingpig.DurkAuto.PC.message.service.ChangeMessageKeyService</class>
	</service>
	
	<!-- 在服务器上线响应处理 -->
	<service>
		<key>OnLineService</key>
		<class>org.lookingpig.DurkAuto.PC.message.service.OnLineServiceBack</class>
	</service>
	
	<!-- 转发服务 -->	
	<service>
		<key>MessageForwardService</key>
		<class>org.lookingpig.DurkAuto.PC.message.service.MessageForwardService</class>
	</service>
	
	<!-- 客户连线服务 -->
	<service>
		<key>OnLine</key>
		<class>org.lookingpig.DurkAuto.PC.message.service.OnLineService</class>
	</service>
	
	<!-- 登陆服务 -->
	<service>
		<key>Login</key>
		<class>org.lookingpig.DurkAuto.PC.message.service.LoginService</class>
	</service>
</message-services>