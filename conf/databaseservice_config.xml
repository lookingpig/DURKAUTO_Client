<?xml version="1.0" encoding="UTF-8"?>
<database-services>
	<!-- 获得用户信息 -->
	<sql>
		<key>GetUserInfo</key>
		<statement>select * from durkauto_user where username=? and password=password(?)</statement>
		<parameter type="string">username</parameter>
		<parameter type="string">password</parameter>
	</sql>
	
	<!-- 预约服务-查询服务类型 -->
	<sql>
		<key>GetAppointmentServiceType</key>
		<statement>select * from durkauto_appointment_type</statement>
	</sql>
</database-services>